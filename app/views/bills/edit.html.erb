<h1>Editing bill</h1>

<% form_for(@bill) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :delivery_date %><br />
    <%= f.date_select :delivery_date %>
  </p>
  <p>
    <%= f.label :billing_date %><br />
    <%= f.date_select :billing_date %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @bill %> |
<%= link_to 'Back', bills_path %>
